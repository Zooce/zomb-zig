{
    "$schema": "vscode://schemas/color-theme",
    "name": "Zooce Dark",
    "type": "Dark",
    "colors": {
        "editor.foreground": "#f2f2f2",
        "editor.background": "#2b2b2b"
    },
    "tokenColors": [
        {
            "scope": "punctuation.definition.arguments",
            "settings": {
                "foreground": "#f2f2f2"
            }
        },
        {
            "scope": "punctuation.definition.type",
            "settings": {
                "foreground": "#ff0000"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#00ff00"
            }
        },
        {
            "scope": "support",
            "settings": {
                "foreground": "#ff0000"
            }
        },
        {
            "scope": "comments",
            "settings": {
                "foreground": "#2b2b2b"
            }
        },
        {
            "scope": "zig.entity",
            "settings": {
                "foreground": "#2b2b2b"
            }
        },
        {
            "scope": "python",
            "settings": {
                "foreground": "#2b2b2b"
            }
        },
        {
            "scope": "c++",
            "settings": {
                "foreground": "#2b2b2b"
            }
        }
    ],
    "multi-line-string": "hello my friends\n what's up with this crazy\nfile format?\"",
    "cool": {
        "ports": [
            "800",
            "900"
        ],
        "this": "this",
        "wh.at": [
            "42",
            {
                "hello": "a",
                "goodbye": [
                    "42",
                    "b"
                ],
                "dude": "Cool"
            }
        ],
        "oh_yea": {
            "thing": "cool = {\n    ports = [ 800 900 ]\n    this = $\"hi.dude\"\n    what = $m1( 0 $m1( a b ) ).goodbye\n    oh_yea = { thing = \"nope\" }\n}\n$hi = this // hi there!\n$m1(one two) = {\n    hello = %one\n    goodbye = %two\n}"
        }
    }
}